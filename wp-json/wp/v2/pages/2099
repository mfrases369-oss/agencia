  border-radius: 25px;\r\n    background-color: #F6FEFD;\r\n    color: #A6A6A6;\r\n    appearance: none;\r\n    -webkit-appearance: none;\r\n    -moz-appearance: none;\r\n    text-align: left;\r\n    margin: 0 auto;\r\n\r\n\">\r\n    <option value=\"0\">Google Ads<\/option>\r\n    <option value=\"1\">Meta Ads<\/option>\r\n     <option value=\"2\">LinkedIn Ads<\/option>\r\n       <option value=\"3\">TikTok Ads<\/option>\r\n        <option value=\"4\">Outros canais<\/option>\r\n<\/select>\r\n<script>\r\n    document.addEventListener(\"DOMContentLoaded\", function() {\r\n        var select = document.getElementById(\"servicos-trafego-pago\");\r\n\r\n        function initializeSwiperControl() {\r\n            var swiperInstance = document.querySelector(\".carrossel-servicos-trafego-pago .swiper\")?.swiper;\r\n\r\n            if (swiperInstance) {\r\n                \/\/ Desativa a navega\u00e7\u00e3o manual por toque e arrasto\r\n                swiperInstance.params.allowTouchMove = false;\r\n                swiperInstance.params.touchRatio = 0;\r\n                swiperInstance.update();\r\n\r\n                \/\/ Adiciona o evento de mudan\u00e7a ao select\r\n                select.addEventListener(\"change\", function() {\r\n                    var selectedValue = parseInt(this.value, 10);\r\n                    if (swiperInstance) {\r\n                        swiperInstance.slideTo(selectedValue, 500); \/\/ 500ms para uma transi\u00e7\u00e3o suave\r\n                    }\r\n                });\r\n            } else {\r\n                \/\/ Tenta novamente caso o Swiper ainda n\u00e3o tenha carregado\r\n                setTimeout(initializeSwiperControl, 500);\r\n            }\r\n        }\r\n\r\n        initializeSwiperControl();\r\n    });\r\n<\/script>\r\n\r\n\r\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-7d979b4 carrossel-servicos-trafego-pago elementor-widget elementor-widget-n-carousel\" data-id=\"7d979b4\" da